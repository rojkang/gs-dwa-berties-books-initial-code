<!DOCTYPE html>
<html>
<head>
    <title>Search Results</title>
</head>
<body>

    <h1>Search Results</h1>

    <% if (results.length === 0) { %>
        <p>No books found.</p>
    <% } else { %>
        <ul>
            <% results.forEach(function(book) { %>
                <li><%= book.name %> — £<%= book.price %></li>
            <% }) %>
        </ul>
    <% } %>

</body>
</html>
